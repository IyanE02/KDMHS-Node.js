<!DOCTYPE html>
<html lang="en">
  <%- include ../header %>
  <body>
    <%- include ../menu %>

    <!-- input -->
    <div class="md-form">
      <input
        type="text"
        id="singer"
        class="form-control"
        value="<%= result.singer %>"
        readonly
      />
      <label for="inputLGEx">SINGER</label>
    </div>

    <div class="md-form">
      <input
        type="text"
        id="title"
        class="form-control"
        value="<%= result.title %>"
        readonly
      />
      <label for="inputLGEx">TITLE</label>
    </div>

    <div class="md-form">
      <input
        type="text"
        id="_id"
        class="form-control"
        value="<%= result._id %>"
        readonly
      />
      <label for="inputLGEx">ID</label>
    </div>
    <button onclick="location.href='/api/music'" class="btn btn-blue btn-md">
      목록
    </button>
    <button
      onclick="location.href='/api/music/<%= result._id %>/edit'"
      class="btn btn-blue btn-md"
    >
      수정
    </button>
    <button class="btn btn-blue btn-md">
      삭제
    </button>
    <%- include ../footer %>
  </body>
</html>
