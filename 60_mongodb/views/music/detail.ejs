    <!-- input -->
    <div class="md-form">
      <input
        type="text"
        id="singer"
        class="form-control"
        value="<%= result.singer %>"
        readonly
      />
      <label for="inputLGEx">SINGER</label>
    </div>

    <div class="md-form">
      <input
        type="text"
        id="title"
        class="form-control"
        value="<%= result.title %>"
        readonly
      />
      <label for="inputLGEx">TITLE</label>
    </div>

    <div class="md-form">
      <input
        type="text"
        id="_id"
        class="form-control"
        value="<%= result._id %>"
        readonly
      />
      <label for="inputLGEx">ID</label>
    </div>

    <%- include ../footer %>
  </body>
</html>
